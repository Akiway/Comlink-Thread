<!-- Main content area -->
<div id="comlink-thread-display">
    <div class="comlink-contacts" data-augmented-ui="tl-clip tr-clip br-clip bl-clip border">
        <h2>Contacts</h2>
        <div class="comlink-contact-list">
            <div class="comlink-contact all">
                <img class="profile-pic" src="data:image/jpeg;base64,">
                <span class="name">Tous les messages</span>
            </div>
            <div class="comlink-contact group">
                <img class="profile-pic" src="data:image/jpeg;base64,">
                <span class="name">Conversation générale</span>
            </div>
            {{#each contacts as |contact|}}
                <div class="comlink-contact {{#if contact.isOffline}}contact-offline{{/if}}" data-sender-id="{{contact.sender._id}}">
                    <div class="profile-pic-wrapper">
                        <img class="profile-pic" src="{{contact.sender.img}}">
                   </div>
                    <span class="name">{{#unless contact.isAnonymous}}{{contact.sender.name}}{{/unless}}{{#if contact.isAnonymous}}Anonymous{{/if}}</span>
                </div>
            {{/each}}
        </div>
    </div>

    <div class="mode-screenshot"> <!-- Div used for screenshot mode -->
        <div class="comlink-container {{#if isGM}}comlink-gm{{/if}}" data-thread-type="all" data-augmented-ui="tl-clip tr-clip br-clip bl-clip border">
            <div class="comlink-thread-actions">
                <a id="comlink-thread-user-page" class="fas fa-address-card user-page-btn" data-tooltip="Ouvrir la fiche du contact"></a>
                {{#if isGM}}<a id="comlink-thread-screenshot" class="fas fa-camera screenshot-btn" data-tooltip="Enregistrer dans le journal"></a>{{/if}}
            </div>
            <div class="comlink-message-list">
                <!-- Your message content -->
                {{#each messages as |message|}}
                    {{#if message.isAdmin}}
                        <div data-recipient-ids="{{join message.recipientIds ','}}" class="message message-{{#if message.isRight}}right{{/if}}{{#unless message.isRight}}left{{/unless}} admin-message">
                            <div class="message-box">
                                <div class="message-content admin-message-content" data-augmented-ui="none">
                                    {{index}}{{message.content}}
                                </div>
                                {{#if ../isGM}}
                                    <div class="message-actions" data-message-id="{{message.id}}">
                                        <div>
                                            <span class="delete-btn fas fa-trash"></span>
                                        </div>
                                        <div class="message-actions-move">
                                            <span class="move-up-btn fas fa-arrow-up"></span>
                                            <span class="move-down-btn fas fa-arrow-down"></span>
                                        </div>
                                    </div>
                                {{/if}}
                            </div>
                        </div>
                    {{/if}}

                    {{#unless message.isAdmin}}
                        <div data-sender-id="{{message.senderId}}" data-recipient-ids="{{join message.recipientIds ','}}"
                             class="message message-{{#if message.isRight}}right{{/if}}{{#unless message.isRight}}left{{/unless}} {{#if message.isQuickAnswerAvailable}}message-answerable{{/if}} {{#if message.isAnonymous}}message-anonymous{{/if}} {{#if message.isOffline}}message-offline{{/if}}">
                            <div class="message-box">
    {{!--                             <div class="message-recipients"> --}}
    {{!--                                 {{#each message.recipients as |recipient|}} --}}
    {{!--                                     <img data-user-id="{{recipient._id}}" class="profile-pic" src="{{recipient.img}}" data-tooltip="Destinataire ({{recipient.name}})"> --}}
    {{!--                                 {{/each}} --}}
    {{!--                             </div> --}}
                                <div class="message-content" data-augmented-ui="{{#if message.isRight}}br-clip tl-2-clip-x{{/if}}{{#unless message.isRight}}bl-clip tr-2-clip-x{{/unless}}">
                                    <div class="message-info">
                                        <span class="name">{{#unless message.isAnonymous}}{{message.sender.name}}{{/unless}}{{#if message.isAnonymous}}Anonymous{{/if}}</span>
                                    </div>
                                    <p>{{message.content}}</p>
                                </div>
                                {{#if message.isQuickAnswerAvailable}}
                                    <div class="response-box" data-message-id="{{message.id}}">
                                        <div class="response-btn accept-btn" data-augmented-ui="br-clip bl-2-clip-x border">
                                            Accepter
                                        </div>
                                        <div class="response-btn refuse-btn" data-augmented-ui="br-2-clip-x bl-clip border">
                                            Refuser
                                        </div>
                                    </div>
                                {{/if}}
                                <div class="profile-pic-wrapper">
                                    <img {{#if ../isGM}}data-user-id="{{message.senderId}}"{{/if}} {{#unless message.isAnonymous}}data-user-id="{{message.senderId}}"{{/unless}} class="profile-pic" src="{{#unless message.isAnonymous}}{{message.sender.img}}{{/unless}}{{#if message.isAnonymous}}data:image/jpeg;base64,{{/if}}">
                                </div>
                                {{#if ../isGM}}
                                    <div class="message-actions" data-message-id="{{message.id}}">
                                        <div>
                                            <span class="delete-btn fas fa-trash"></span>
                                            {{#if message.isAnonymous}}
                                                <span class="toggle-anonymous-btn active fas fa-user-secret"></span>
                                            {{/if}}
                                            {{#unless message.isAnonymous}}
                                                <span class="toggle-anonymous-btn fas fa-user-secret"></span>
                                            {{/unless}}
                                            {{#if message.isOffline}}
                                                <span class="toggle-offline-btn active fas fa-signal-slash"></span>
                                            {{/if}}
                                            {{#unless message.isOffline}}
                                                <span class="toggle-offline-btn fas fa-signal-slash"></span>
                                            {{/unless}}
                                        </div>
                                        <div class="message-actions-move">
                                            <span class="move-up-btn fas fa-arrow-up"></span>
                                            <span class="move-down-btn fas fa-arrow-down"></span>
                                        </div>
                                    </div>
                                {{/if}}
                            </div>
                        </div>
                    {{/unless}}
                {{/each}}
            </div>
        </div>
    </div>
</div>